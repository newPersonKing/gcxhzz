<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingLeft="@dimen/dp_15">

    <TextView
        android:id="@+id/zaxcd"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="左岸岸线长度"
        android:layout_marginTop="@dimen/dp_20"/>
    <TextView
        android:id="@+id/yaxcd"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="右岸岸线长度"
        android:layout_marginTop="@dimen/dp_20"/>
    <TextView
        android:id="@+id/jxzaxcd"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="江心洲岸线长度"
        android:layout_marginTop="@dimen/dp_20"/>
    <TextView
        android:id="@+id/hjaxcd"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="合计岸线长度"
        android:layout_marginTop="@dimen/dp_20"/>
    <TextView
        android:id="@+id/bhqaxgnqfcd"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="保护区岸线功能分区长度"
        android:layout_marginTop="@dimen/dp_20"/>

    <TextView
        android:id="@+id/blqaxgnqfcd"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="保留区岸线功能分区长度"
        android:layout_marginTop="@dimen/dp_20"/>
    <TextView
        android:id="@+id/kzlyqaxgnfqcd"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="控制利用区岸线功能分区长度"
        android:layout_marginTop="@dimen/dp_20"/>
    <TextView
        android:id="@+id/kflyqaxgnfqcd"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="开发利用区岸线功能分区长度"
        android:layout_marginTop="@dimen/dp_20"/>
    <TextView
        android:id="@+id/kflycd"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="开发利用长度"
        android:layout_marginTop="@dimen/dp_20"/>
    <TextView
        android:id="@+id/kflyl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="开发利用率"
        android:layout_marginTop="@dimen/dp_20"/>
</LinearLayout>
